  <!-- ‚ïê‚ïê CODE SHOWCASE ‚ïê‚ïê -->
  <div class="sec-head">
    <h2>Methods & Code</h2>
    <div class="sec-rule"></div>
    <span class="sec-badge">Technical Depth</span>
  </div>

  <div class="card-grid card-grid-2" data-section="code">

    <div class="code-card" data-cat="code model">
      <div class="code-card-header">
        <div class="code-dots">
          <div class="code-dot r"></div>
          <div class="code-dot y"></div>
          <div class="code-dot g"></div>
        </div>
        <div class="code-lang">STATA ¬∑ Discrete-Time Survival Model</div>
      </div>
      <div class="code-body">
<span class="c-comment">// Discrete-Time Hazard Model ‚Äî Depression Onset</span>
<span class="c-comment">// HPS Cycles 1-9 | Survey-Weighted | JD-R Framework</span>

<span class="c-keyword">svyset</span> [<span class="c-option">pw</span>=pweight]

<span class="c-comment">// Full JD-R Model with interaction</span>
<span class="c-keyword">svy</span>: <span class="c-fn">logit</span> dep_first_event i.cycle ///
    dem_fininst not_married ///
    i.<span class="c-var">work_arrangement</span>##i.<span class="c-var">social3</span> ///
    i.age_group i.gender i.race_ethnicity ///
    i.education i.ann_income i.region

<span class="c-comment">// Test interaction significance</span>
<span class="c-fn">testparm</span> i.<span class="c-var">work_arrangement</span>#i.<span class="c-var">social3</span>

<span class="c-comment">// Marginal effects at representative values</span>
<span class="c-fn">margins</span> work_arrangement, ///
    <span class="c-option">at</span>(social3=(<span class="c-num">1 2 3</span>)) ///
    <span class="c-option">vce</span>(unconditional) post
<span class="c-fn">marginsplot</span>
      </div>
    </div>

    <div class="code-card" data-cat="code model">
      <div class="code-card-header">
        <div class="code-dots">
          <div class="code-dot r"></div>
          <div class="code-dot y"></div>
          <div class="code-dot g"></div>
        </div>
        <div class="code-lang">SAS ¬∑ Regression with Stepwise Selection</div>
      </div>
      <div class="code-body">
<span class="c-comment">/* Tuition Pricing Model ‚Äî GLMSELECT */</span>
<span class="c-comment">/* Train/Test Split ¬∑ Adj R¬≤ Selection */</span>

<span class="c-keyword">PROC GLMSELECT</span> Data=proj_impute
    Plots=All Seed=<span class="c-num">523654</span>;
  <span class="c-keyword">CLASS</span> parochial;
  <span class="c-fn">Partition</span> fraction(Test=<span class="c-num">0.4</span>);
  <span class="c-keyword">MODEL</span> tuition =
    top25 sf_ratio fac_comp collrate
    graduat pct_phd fulltime alumni
    num_enrl parochial /
    Selection=<span class="c-fn">Stepwise</span>(
      Select=SL SLE=<span class="c-num">0.15</span> SLS=<span class="c-num">0.15</span>
      Choose=<span class="c-option">AdjRSQ</span>)
    Details=ALL Hierarchy=Single
    Stats=ALL showpvalues;
<span class="c-keyword">RUN</span>;

<span class="c-comment">/* Final model: Adj R¬≤ = 0.71 */</span>
<span class="c-comment">/* F = 392.19, p &lt; .0001 */</span>
      </div>
    </div>

    <div class="code-card" data-cat="code model">
      <div class="code-card-header">
        <div class="code-dots">
          <div class="code-dot r"></div>
          <div class="code-dot y"></div>
          <div class="code-dot g"></div>
        </div>
        <div class="code-lang">R ¬∑ Salary Comparative Analysis</div>
      </div>
      <div class="code-body">
<span class="c-comment"># USG Higher Ed Salary Analysis</span>
<span class="c-comment"># Kruskal-Wallis + YOY % Change</span>

<span class="c-keyword">library</span>(<span class="c-str">sqldf</span>); <span class="c-keyword">library</span>(<span class="c-str">stringr</span>)

<span class="c-comment"># Non-parametric test by title</span>
<span class="c-fn">kruskal.test</span>(SalaryNum ~
  <span class="c-fn">as.factor</span>(Fiscal.Year), data = PROFassist)

<span class="c-comment"># YOY % change per position</span>
PROFassistyoy <- (<span class="c-fn">mean</span>(
  PROFassist12$SalaryNum, na.rm=<span class="c-num">TRUE</span>) -
  <span class="c-fn">mean</span>(PROFassist11$SalaryNum, na.rm=<span class="c-num">TRUE</span>)) /
  <span class="c-fn">mean</span>(PROFassist11$SalaryNum, na.rm=<span class="c-num">TRUE</span>)

<span class="c-comment"># Boxplot with jitter overlay</span>
<span class="c-fn">boxplot</span>(SalaryNum~Organization,
  data=PROFassist, outline=<span class="c-num">FALSE</span>,
  col=<span class="c-fn">c</span>(<span class="c-str">"royalblue2"</span>,<span class="c-str">"red3"</span>,<span class="c-str">"gold"</span>,<span class="c-str">"gray"</span>,<span class="c-str">"red"</span>))
      </div>
    </div>

    <div class="code-card" data-cat="code">
      <div class="code-card-header">
        <div class="code-dots">
          <div class="code-dot r"></div>
          <div class="code-dot y"></div>
          <div class="code-dot g"></div>
        </div>
        <div class="code-lang">SAS ¬∑ ARIMA Time Series Forecasting</div>
      </div>
      <div class="code-body">
<span class="c-comment">/* ARIMA Demand Forecast Model */</span>
<span class="c-comment">/* 6-month horizon ¬∑ Resource Planning */</span>

<span class="c-keyword">PROC ARIMA</span> data=call_volume;
  <span class="c-fn">IDENTIFY</span> var=daily_calls
    nlag=<span class="c-num">30</span> stationarity=(adf);

  <span class="c-fn">ESTIMATE</span> p=<span class="c-num">1</span> q=<span class="c-num">1</span>
    input=(<span class="c-num">1</span> $ exception_flag)
    method=ML;

  <span class="c-fn">FORECAST</span>
    lead=<span class="c-num">180</span>
    interval=day
    id=date
    out=forecast_out;
<span class="c-keyword">RUN</span>;
<span class="c-keyword">QUIT</span>;

<span class="c-comment">/* Transfer function captures */</span>
<span class="c-comment">/* external event impact */</span>
      </div>
    </div>

  </div>

