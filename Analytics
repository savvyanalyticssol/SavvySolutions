// Filter functionality
function filterCards(cat, btn) {
  document.querySelectorAll('.filter-pill').forEach(p => p.classList.remove('active'));
  btn.classList.add('active');
  const cards = document.querySelectorAll('[data-cat]');
  cards.forEach(card => {
    if (cat === 'all') {
      card.classList.remove('hidden');
    } else {
      const cats = card.getAttribute('data-cat') || '';
      if (cats.includes(cat)) {
        card.classList.remove('hidden');
      } else {
        card.classList.add('hidden');
      }
    }
  });
}

// ROI Calculator
function calcROI() {
  const size = parseFloat(document.getElementById('wf-size').value) || 500;
  const pct = parseFloat(document.getElementById('cg-pct').value) || 0.20;
  const sal = parseFloat(document.getElementById('avg-sal').value) || 65000;

  // Strain distribution from NSOC research (approximate)
  const caregivers = size * pct;
  const lowStrain = caregivers * 0.40;
  const modStrain = caregivers * 0.35;
  const highStrain = caregivers * 0.25;

  // Productivity loss estimates by strain level
  const presenteeismPct = { low: 0.05, mod: 0.12, high: 0.22 };
  const loss =
    (lowStrain * sal * presenteeismPct.low) +
    (modStrain * sal * presenteeismPct.mod) +
    (highStrain * sal * presenteeismPct.high);

  const fmt = new Intl.NumberFormat('en-US', { style: 'currency', currency: 'USD', maximumFractionDigits: 0 });
  document.getElementBy
